{
  "name": "lunch-admin-app",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/api/**": {
      "target": "http://localhost:3005"
    }
  },
  "dependencies": {
    "auth0-js": "9.8.2",
    "history": "^4.7.2",
    "react": "^16.3.2",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start cypress": "REACT_APP_MOCK=true react-scripts start",
    "start-now": "serve --single ./build",
    "build": "react-scripts build",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "now -e NODE_ENV=production --token $NOW_TOKEN --public",
    "alias": "now alias --token=$NOW_TOKEN"
  },
  "devDependencies": {
    "cypress": "3.1.2",
    "flow-bin": "0.72.0",
    "now": "12.1.12",
    "serve": "6.5.7",
    "styled-components": "3.2.6",
    "chai": "4.2.0"
  }
}
